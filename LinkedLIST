LINKED LIST JAVA REPRESENTATION

class Node{
    int data;
    Node next;
}
class Main {
    public static void main(String[] args) {
        //System.out.println("Try programiz.pro");
        Node node1 = new Node();
        Node node2 = new Node();
        Node node3 = new Node();
        Node node4 = new Node();
        Node node5 = new Node();
        
        node1.data = 1;
        node1.next = node2;
        
        
        node2.data = 2;
        node2.next = node3;
        
        node3.data = 3;
        node3.next = node4;
        
        node4.data = 4;
        node4.next = node5;
        
        node5.data = 5;
        node5.next = null;
        
        System.out.println(node1.next.next.next.data);
        
        

    }
}
